getNextWord <- function(string) {
string <- gsub("[^[:alnum:] ]","", tolower(string))
wordVector <- as.data.frame(strsplit(string, " "))
wordVector[,1] <- as.character(wordVector[,1])
n <- dim(wordVector)[1]
lastTwo <- paste(wordVector[n-1,1], wordVector[n,1], sep = "_")
lastOne <- wordVector[n,1]
nextWord <- ""
if (lastTwo %in% tri$firstTerms) {
triSub <- subset(tri, firstTerms == lastTwo)
nextWord <- sample(triSub$lastTerm, size = 1, replace = TRUE, prob = triSub$prb)
} else if (lastOne %in% bi$firstTerms) {
biSub <- subset(bi, firstTerms == lastOne)
nextWord <- sample(biSub$lastTerm, size = 1, replace = TRUE, prob = biSub$prb)
} else {
nextWord <- sample(uni$lastTerm, size = 1, replace = TRUE, prob = uni$prb)
}
return(nextWord)
}
